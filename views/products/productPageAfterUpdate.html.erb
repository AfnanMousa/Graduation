    <div class="o_action_manager">
        <div class="o_action o_view_controller o_view_sample_data">
            <div data-command-category="actions" class="o_control_panel">
                <div class="o_cp_top">
                    <div class="o_cp_top_left">
                        <ol role="navigation" class="breadcrumb">
                            <li class="breadcrumb-item active"><span class="text-900">Products</span></li>
                        </ol>
                    </div>
                    <div class="o_cp_top_right">
                        <div role="search" class="o_cp_searchview">
                            <div role="search" aria-autocomplete="list" class="o_searchview"><i title="Search..."
                                    role="img" aria-label="Search..." class="o_searchview_icon fa fa-search"></i>
                                <div class="o_searchview_input_container">
                                    <div tabindex="0" role="img" aria-label="search" class="o_searchview_facet"><span
