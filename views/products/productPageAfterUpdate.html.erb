                                <button type="button" class="btn btn-primary o-kanban-button-new" title="Create record"
                                    accesskey="c">
                                    <!--Create-->

                                    <a href="addProduct" class="btn btn-primary o-kanban-button-new">Create</a>

                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="o_cp_bottom_right">
                        <div role="search" class="btn-group o_search_options position-static">
                            <div class="o-dropdown dropdown o-dropdown--no-caret o_filter_menu btn-group"><button
                                    class="dropdown-toggle btn btn-light "><i class="small mr-1 fa fa-filter"></i><span
                                        class="o_dropdown_title">Filters</span></button></div>
                            <div class="o-dropdown dropdown o-dropdown--no-caret o_group_by_menu btn-group"><button
                                    class="dropdown-toggle btn btn-light "><i class="small mr-1 fa fa-bars"></i><span
                                        class="o_dropdown_title">Group By</span></button></div>
                            <div class="o-dropdown dropdown o-dropdown--no-caret o_favorite_menu btn-group"><button
                                    class="dropdown-toggle btn btn-light "><i class="small mr-1 fa fa-star"></i><span
                                        class="o_dropdown_title">Favorites</span></button></div>
                        </div>
                        <div role="search" class="o_cp_pager"></div>
                        <nav role="toolbar" aria-label="View switcher" class="btn-group o_cp_switch_buttons"><button
                                type="button" class="btn btn-light fa fa-lg o_switch_view o_kanban fa-th-large active"
                                aria-label="View kanban" data-tooltip="Kanban" tabindex="-1"></button><button
                                type="button" class="btn btn-light fa fa-lg o_switch_view o_list fa-list-ul"
                                aria-label="View list" data-tooltip="List" tabindex="-1"></button></nav>
                    </div>
                </div>
            </div>

            <div class="o_content">
                <div class="o_kanban_view o_kanban_ungrouped">
                    <div class="oe_kanban_card oe_kanban_global_click o_kanban_record" modifiers="{}" tabindex="0"
                        role="article">
                        <div class="o_kanban_image mr-1" modifiers="{}">
                            <img src="https://student67.odoo.com/web/image?model=product.template&amp;field=image_128&amp;id=2&amp;unique=04012022002106"
                                alt="Product" class="o_image_64_contain" modifiers="{}">
                        </div>
                        <div class="oe_kanban_details" modifiers="{}">
                            <div class="o_kanban_record_top mb-0" modifiers="{}">
                                <div class="o_kanban_record_headings" modifiers="{}">
                                    <strong class="o_kanban_record_title" modifiers="{}">
                                        <span>afnan</span>
                                    </strong>
                                </div>
                                <div role="radiogroup" class="o_priority o_field_widget" name="priority"
                                    aria-label="Favorite"><a href="#" role="radio" aria-checked="false"
                                        title="Favorite: Favorite" aria-label="Favorite: Favorite" tabindex="0"
                                        data-index="1" class="o_priority_star fa fa-star-o"></a></div>
                            </div>


                            <div name="product_lst_price" class="mt-1" modifiers="{}">
                                Price: <span class="o_field_monetary o_field_number o_field_widget o_quick_editable"
                                    name="list_price">1.00&nbsp;E£</span>
                            </div>
                            <div modifiers="{}">On hand: <span>0.00</span> <span>Units</span></div>
                        </div>
                    </div>
                    <div class="oe_kanban_card oe_kanban_global_click o_kanban_record" modifiers="{}" tabindex="0"
